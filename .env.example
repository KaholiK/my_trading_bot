# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# LLM Provider Configuration
MODEL_PROVIDER=openai
# MODEL_PROVIDER=mock  # Use for testing without API calls
MODEL_API_KEY=sk-your-openai-api-key

# Application URLs
APP_URL_WEB=http://localhost:3000
APP_URL_API=http://localhost:3001

# Access Control
ALLOWLIST_EMAILS=user1@example.com,user2@example.com

# Rate Limiting (JSON format)
RATE_LIMITS_JSON={"perUserPerDay":500}

# Feature Flags
FEATURE_DRIVER_MODE=false

# Node Environment
NODE_ENV=development
